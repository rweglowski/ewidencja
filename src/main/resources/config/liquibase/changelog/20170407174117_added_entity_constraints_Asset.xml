<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">
    <!--
        Added the constraints for entity Asset.
    -->
    <changeSet id="20170407174117-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="assets_id"
                                 baseTableName="asset_employee"
                                 constraintName="fk_asset_employee_assets_id"
                                 referencedColumnNames="id"
                                 referencedTableName="asset"/>
        <addForeignKeyConstraint baseColumnNames="employees_id"
                                 baseTableName="asset_employee"
                                 constraintName="fk_asset_employee_employees_id"
                                 referencedColumnNames="id"
                                 referencedTableName="employee"/>
        
    </changeSet>
</databaseChangeLog>
